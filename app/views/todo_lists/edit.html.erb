<h2>Edit Todo List</h2>

<%= form_with model: @todo_list, local: true do |form| %>
  <div>
    <%= form.label :title, 'Title' %>
    <%= form.text_field :title, required: true %>
  </div>
  <div>
    <%= form.submit 'Update Todo List' %>
  </div>
<% end %>

<%= link_to 'Back to Lists', todo_lists_path %>
